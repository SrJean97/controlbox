import { ModuleWithProviders, Type } from '@angular/core';
import { Profile } from '@dynamic-mapper/mapper';
export declare function createProfileProvider(profile: any): {
    provide: import("@angular/core").InjectionToken<Profile>;
    useClass: any;
    multi: boolean;
};
export declare class MapperModule {
    static withProfiles(profiles: Type<Profile>[]): ModuleWithProviders<MapperModule>;
}
